<div class="container">
 <div class="row">
  <div class="col-md-12">
   <h1>{{title}}</h1>
   <ul>
    {{#each physicians}}
    <li>
      <div class="physician-result">
        <div class="img-wrapper">
          <a href="/physicians/{{this.url_path}}">
            {{#if this.prof_pic}}
            <img class="profile-pic" src="/profiles/physician/{{this.prof_pic}}">
            {{else}}
            <img class="not-found" src="">
            {{/if}}
          </a>
        </div>
        <div class="body">
          <h3>
            <a href="/physicians/{{this.url_path}}">
              {{this.id}}. Dr. {{this.first_name}} {{this.last_name}}
            </a>
          </h3>
          <div class="details">
            {{this.specialty}}
          </div>
        </div>
      </div>
    </li>
    {{/each}}
   </ul>
  </div>
 </div>
</div>
